
generator client {
  provider = "prisma-client-js"
}

datasource db {
  // Indicar el tipo de base de datos (mysql)
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Producto {
  id        Int      @id @default(autoincrement())
  name      String
  category  String
  quantity  Int
  price     Float
}